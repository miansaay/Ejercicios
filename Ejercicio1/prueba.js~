

var movie = {title: 'The Godfather', 'releaseInfo': {'year': 1972, rating: 'PG'}}

for (i in movie) { 
        console.log(i + ":")
        if (i == "releaseInfo"){
                for (j in movie.releaseInfo){
                        console.log(movie.releaseInfo[j])
                }
        } else {
                console.log(movie.title)
        }
}
